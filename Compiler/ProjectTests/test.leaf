def Vec2: struct {
	x: i8;
	y: i8;
}

@VarArgs
@External
def printf: (str: i8*) -> i32;

def toString: (this: ref Vec2) {
	printf(&"(%i, %i)", x, y);
	return;
}

def freeThis: (this: mut ref Vec2) {
	free this;
	return;
}

def main: () -> i8 {
	var vec = Vec2 {x = 42, y = 64};
	vec.freeThis();
	return 0;	
}